@model IEnumerable<Examino.Models.Entities.UserAnswer>

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Question.QuestionLabel)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.UserQuiz.ApplicationUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsUserAnswer)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Development)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Point)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Question.QuestionLabel)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.UserQuiz.ApplicationUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsUserAnswer)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Development)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Point)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Id }) |
            @Html.ActionLink("Details", "Details", new { id=item.Id }) |
            @Html.ActionLink("Delete", "Delete", new { id=item.Id })
        </td>
    </tr>
}

</table>
